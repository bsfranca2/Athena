{
  "baseUrl": ".",
  "include": ["src/**/*", "tests/**/*"],
  "compilerOptions": {
    "baseUrl": ".",
    "target": "esnext",
    "module": "es2015",
    "paths": {
      "@/*": ["./*"],
      "@": [
        "./index.ts",
        "./index.js",
        "./index.json",
        "./index.vue",
        "./index.scss",
        "./index.css"
      ],
      "@src/*": ["src/*"],
      "@src": [
        "src/index.ts",
        "src/index.js",
        "src/index.json",
        "src/index.vue",
        "src/index.scss",
        "src/index.css"
      ],
      "@router/*": ["src/router/*"],
      "@router": [
        "src/router/index.ts",
        "src/router/index.js",
        "src/router/index.json",
        "src/router/index.vue",
        "src/router/index.scss",
        "src/router/index.css"
      ],
      "@views/*": ["src/router/views/*"],
      "@views": [
        "src/router/views/index.ts",
        "src/router/views/index.js",
        "src/router/views/index.json",
        "src/router/views/index.vue",
        "src/router/views/index.scss",
        "src/router/views/index.css"
      ],
      "@layouts/*": ["src/router/layouts/*"],
      "@layouts": [
        "src/router/layouts/index.ts",
        "src/router/layouts/index.js",
        "src/router/layouts/index.json",
        "src/router/layouts/index.vue",
        "src/router/layouts/index.scss",
        "src/router/layouts/index.css"
      ],
      "@components/*": ["src/components/*"],
      "@components": [
        "src/components/index.ts",
        "src/components/index.js",
        "src/components/index.json",
        "src/components/index.vue",
        "src/components/index.scss",
        "src/components/index.css"
      ],
      "@assets/*": ["src/assets/*"],
      "@assets": [
        "src/assets/index.ts",
        "src/assets/index.js",
        "src/assets/index.json",
        "src/assets/index.vue",
        "src/assets/index.scss",
        "src/assets/index.css"
      ],
      "@utils/*": ["src/utils/*"],
      "@utils": [
        "src/utils/index.ts",
        "src/utils/index.js",
        "src/utils/index.json",
        "src/utils/index.vue",
        "src/utils/index.scss",
        "src/utils/index.css"
      ],
      "@store/*": ["src/store/*"],
      "@store": [
        "src/store/index.ts",
        "src/store/index.js",
        "src/store/index.json",
        "src/store/index.vue",
        "src/store/index.scss",
        "src/store/index.css"
      ],
      "@services/*": ["src/services/*"],
      "@services": [
        "src/services/index.ts",
        "src/services/index.js",
        "src/services/index.json",
        "src/services/index.vue",
        "src/services/index.scss",
        "src/services/index.css"
      ],
      "@models/*": ["src/models/*"],
      "@models": [
        "src/models/index.ts",
        "src/models/index.js",
        "src/models/index.json",
        "src/models/index.vue",
        "src/models/index.scss",
        "src/models/index.css"
      ],
      "@scss/*": ["src/assets/scss/index.scss/*"],
      "@scss": ["src/assets/scss/index.scss"]
    }
  }
}
